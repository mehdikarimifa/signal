{
   "extends": "@vue/tsconfig/tsconfig.web.json",
   "include": [
      "env.d.ts",
      "src/**/*",
      "src/**/*.vue"
   ],
   "exclude": [
      "node_modules"
   ],
   "compilerOptions": {
      "baseUrl": ".",
      "paths": {
         "@/*": [
            "./src/*"
         ]
      },
      "allowJs": true
   },
   "references": [
      {
         "path": "./tsconfig.vite-config.json"
      }
   ]
}
